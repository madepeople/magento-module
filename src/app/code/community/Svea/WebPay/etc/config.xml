<?xml version="1.0" encoding="UTF-8"?>
<config>
    <modules>
        <Svea_WebPay>
            <version>4.1.0</version>
        </Svea_WebPay>
    </modules>
    <global>
        <models>
            <svea_webpay>
                <class>Svea_WebPay_Model</class>
            </svea_webpay>
        </models>
    </global>
    <default>
        <payment>
            <svea_invoice>
                <active>0</active>
                <model>svea_webpay/payment_service_invoice</model>
                <title>Svea Invoice</title>
                <test>1</test>
                <payment_action>authorize</payment_action>
            </svea_invoice>
        </payment>
    </default>
</config>